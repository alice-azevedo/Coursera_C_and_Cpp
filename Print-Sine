#include <stdio.h>
#include <math.h>

int main(void) {

    double x;
    printf("Enter a value between 0 and 1: ");
    scanf("%lf", &x);

    if (x > 0 && x < 1) {
        double sine_value = sin(x);
        printf("The sine of %lf is: %lf\n", x, sine_value);
    }
    else {
        printf("Please enter a valid number\n");
    }

    return 0;
}
